<form class="logInForm">
  <h1><label for="username">Username</label></h1>
  <input type="text" name="username" id="loginUsername" placeholder="__XXXXasdXXXX__">

  <h1><label for="password">Password</label></h1>
  <input type="password" name="password" id="loginPassword" placeholder="*********">
  <br>
  <button type="submit" value="Submit" class="submitButton">Submit</button>


  <script src="/js/login.js"></script>
</form>